{
  "name": "cables-daplaya",
  "version": "0.0.8",
  "license": "MIT",
  "description": "a standalone player for cables.gl-patches using the electron framework\n\n",
  "author": "undefined development <hi@undev.de>",
  "repository": "github:cables-gl/cables-daplaya",
  "keywords": [
    "cables",
    "cables.gl",
    "webgl",
    "daplaya",
    "electron"
  ],
  "main": "./app",
  "scripts": {
    "app": "electron ./app",
    "postinstall": "install-app-deps",
    "start": "npm install && electron ./app",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -wml",
    "host": "electron-builder",
    "release": "electron-builder publish"
  },
  "files": [
    "**/*",
    "build/icon.*"
  ],
  "build": {
    "appId": "gl.cables.daplaya",
    "productName": "daPlaya",
    "mac": {
      "category": "public.app-category.graphics-design",
      "target": [
        "dmg"
      ],
      "publish": [
        "github"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "publish": [
        "github"
      ]
    },
    "win": {
      "target": [
        "portable"
      ],
      "icon": "build/icon.png",
      "publish": [
        "github"
      ]
    }
  },
  "dependencies": {},
  "devDependencies": {
    "electron": "5.0.8",
    "electron-builder": "21.1.5",
    "electron-packager": "14.0.3",
    "nwjs-builder-phoenix": "^1.15.0"
  }
}
